//
// Menu
//

IDR_MAINFRAME MENU PRELOAD DISCARDABLE 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建(&N)",                    IDM_NEWFILE
        MENUITEM "打开(&O)",                    IDM_OPENFILE
        MENUITEM "保存(&S)",                    IDM_SAVEFILE
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                    ID_APP_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "导入(&I)",                    IDM_IMPORTIMG
        MENUITEM "导出(&E)",                    IDM_EXPORTIMG
        MENUITEM "批量导入(&P)",                IDM_BATCHIMPORTIMG
        MENUITEM "批量导出(&Q)",                IDM_BATCHEXPORTIMG
        MENUITEM SEPARATOR
        MENUITEM "修改偏移(&M)",                IDM_MODOFFSET
        MENUITEM "删除(&D)",                    IDM_DELIMG
        MENUITEM "剪切(&C)",                    IDM_CUT
        MENUITEM "粘贴(&T)",                    IDM_PASTE
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "透明显示(&T)",                IDM_DRAWTRANSPARENT, CHECKED
        MENUITEM "显示坐标线(&C)",              IDM_DRAWCOORD, CHECKED
        MENUITEM "显示偏移(&O)",                IDM_DRAWOFFSET
        MENUITEM SEPARATOR
        MENUITEM "上一张(&P)",                  IDM_PREIMG
        MENUITEM "下一张(&N)",                  IDM_NEXTIMG
        MENUITEM "播放(&A)",                    IDM_AUTOPLAY
        MENUITEM "暂停(&S)",                    IDM_PAUSEIMG
        MENUITEM "跳转(&G)",                    IDM_GOTOIMG
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 BilEditor(&A)...",       ID_APP_ABOUT
    END
END


